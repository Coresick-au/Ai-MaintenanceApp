rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Helper function
    function isSignedIn() {
      return request.auth != null;
    }

    // Match all files
    match /{allPaths=**} {
      // Only allow access if user is authenticated
      allow read, write: if isSignedIn();
    }
  }
}
